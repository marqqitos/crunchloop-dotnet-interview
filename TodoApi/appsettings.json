{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ExternalApi": {
    "BaseUrl": "http://localhost:8080",
    "TimeoutSeconds": 30,
    "SourceId": "local-api-v1"
  },
  "RetryPolicy": {
    "MaxRetryAttempts": 3,
    "BaseDelayMs": 1000,
    "MaxDelayMs": 30000,
    "JitterFactor": 0.1,
    "RequestTimeoutSeconds": 30,
    "EnableRetries": true
  }
}